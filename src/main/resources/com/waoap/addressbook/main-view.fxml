    <Button fx:id="addButton" layoutX="360.0" layoutY="15.0" mnemonicParsing="false" text="新增"/>
    <Text layoutX="15.0" layoutY="626.0" strokeType="OUTSIDE" strokeWidth="0.0" text="操作日志："/>
    <Text fx:id="logField" layoutX="135.0" layoutY="626.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="RIGHT"
          wrappingWidth="280.0"/>
    <VBox fx:id="navigationList" layoutX="428.0" layoutY="60.0" prefHeight="495.0" prefWidth="8.0">
        <Label text="#"/>
        <Label text="A"/>
        <Label text="B"/>
        <Label text="C"/>
        <Label text="D"/>
        <Label text="E"/>
        <Label text="F"/>
        <Label text="G"/>
        <Label text="H"/>
